@model MOE.Common.Business.FilterExtensions.FilterSignal

<div class="panel panel-signal">
    <div class="panel-heading">
        <h5 class="panel-title">
            <a id="Signals" data-toggle="collapse" href="#collapseSignal_@Model.VersionId">
                @Html.DisplayFor(model => model.Description)
            </a>
            <a class="pull-right">
                <span>
                    @Html.LabelFor(m => m.Exclude)
                    @Html.CheckBoxFor(m => m.Exclude)
                </span>
            </a>
        </h5>
    </div>
<div id="collapseSignal_@Model.VersionId" class="panel-body panel-collapse collapse">
        @Html.HiddenFor(m => m.SignalId)
        @Html.HiddenFor(m => m.VersionId)
        @Html.DisplayFor(m => m.FilterApproaches)
    </div></div>