@model MOE.Common.Models.ViewModel.LinkPivotPCDsViewModel

<div class="row">
    <div class="col-md-6">
        <h3>Existing</h3>
        <div class="row">
            <div class="col-md-12">
                <label class="control-label" for="uxExistingAOGLabel">Total # AOG - </label>
                @Html.DisplayFor(m => m.ExistingAog)
            </div>
        </div>
        <div class="row">
            <div class="col-md-12">
                <label class="control-label" for="uxExistingPAOGLabel">Total % AOG - </label>
                @Html.DisplayFor(m => m.ExistingAogPercent)
            </div>
        </div>
        <div class="row">
            <div class="col-md-12">
                <a href="@Model.ExistingChart" alt="Enlarged Existing Chart" target="_blank"><img class="ImageHome" src="@Model.ExistingChart" alt="@Model.UpstreamBeforeTitle " /></a>
            </div>
        </div>
        <div class="row">
            <div class="col-md-12">
                <a href="@Model.ExistingDownChart" alt="Enlarged Existing Down Chart" target="_blank"><img class="ImageHome" src="@Model.ExistingDownChart" alt="@Model.DownstreamBeforeTitle " /></a>
            </div>
        </div>
    </div>
    <div class="col-md-6">
        <h3>Predicted</h3>
        <div class="row">

            <div class="col-md-12">
                <label class="control-label" for="uxPredictedAOGLabel">Total # AOG - </label>
                @Html.DisplayFor(m => m.PredictedAog)
            </div>
        </div>
        <div class="row">
            <div class="col-md-12">
                <label class="control-label" for="uxPredictedPAOGLabel">Total % AOG - </label>
                @Html.DisplayFor(m => m.PredictedAogPercent)
            </div>
        </div>
        <div class="row">
            <div class="col-md-12">
                <a href="@Model.PredictedChart" alt="Enlarged Predicted Chart" target="_blank">
                    <img class="ImageHome" src="@Model.PredictedChart" alt="@Model.UpstreamAfterTitle" /></a>
            </div>
        </div>
        <div class="row">
            <div class="col-md-12">
                <a href="@Model.PredictedDownChart" alt="Enlarged Predicted Down Chart" target="_blank">
                    <img class="ImageHome" src="@Model.PredictedDownChart" alt="@Model.DownstreamAfterTitle" />
                    </a>
</div>
        </div>
    </div>
</div>
